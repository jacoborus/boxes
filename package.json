{
  "name": "boxes",
  "version": "0.19.6",
  "description": "Mutable state containers with time travelling",
  "main": "boxes.js",
  "scripts": {
    "linter": "standard boxes.js test/tests.js",
    "test": "node ./test/tests.js && npm run linter",
    "build-umd": "rollup --config",
    "build-tests": "mkdir test/dist -p && browserify test/tests.js -o test/dist/tests.js",
    "build": "npm run build-umd && npm run build-tests"
  },
  "engines": {
    "node": ">=5.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jacoborus/boxes.git"
  },
  "keywords": [
    "state",
    "mutable",
    "containers",
    "undo",
    "redo",
    "history",
    "time travel"
  ],
  "author": "Jacobo Tabernero - https://github.com/jacoborus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacoborus/boxes/issues"
  },
  "homepage": "https://github.com/jacoborus/boxes",
  "devDependencies": {
    "browserify": "^13.1.0",
    "rollup": "^0.35.9",
    "rollup-plugin-commonjs": "^4.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "standard": "^8.0.0",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "arbitrary-emitter": "^1.0.0"
  }
}
