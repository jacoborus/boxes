{
  "name": "boxes",
  "version": "0.16.3",
  "description": "Mutable state containers with time travelling",
  "main": "boxes.js",
  "scripts": {
    "linter": "eslint boxes.js test/tests.js",
    "test": "node ./test/tests.js",
    "build-umd": "rm -rf dist && mkdir dist && browserify boxes.js -s boxes -o dist/boxes.umd.js",
    "build-tests": "mkdir test/dist -p && browserify test/tests.js -o test/dist/tests.js",
    "build": "npm run build-umd && npm run build-tests",
    "postinstall": "npm run build-umd"
  },
  "engines": {
    "node": ">=5.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jacoborus/boxes.git"
  },
  "keywords": [
    "state",
    "mutable",
    "containers",
    "undo",
    "redo",
    "history",
    "time travel"
  ],
  "author": "Jacobo Tabernero - https://github.com/jacoborus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacoborus/boxes/issues"
  },
  "homepage": "https://github.com/jacoborus/boxes",
  "devDependencies": {
    "browserify": "^13.0.0",
    "eslint": "^2.6.0",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "arbitrary-emitter": "^0.7.6"
  }
}
