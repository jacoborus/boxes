{
  "name": "boxes",
  "version": "0.32.0",
  "description": "Reactive state containers for js apps",
  "type": "module",
  "main": "lib/boxes.js",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "dist": "rollup --config",
    "compress": "terser -c -m -o bundle.min.js -- bundle.js",
    "linter": "eslint src/**/**.ts",
    "prepublish": "rm -rf lib && tsc",
    "test": "tape -r ts-node/register src/tests.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jacoborus/boxes.git"
  },
  "keywords": [
    "reactive",
    "state",
    "containers"
  ],
  "author": "Jacobo Tabernero - https://github.com/jacoborus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacoborus/boxes/issues"
  },
  "homepage": "https://github.com/jacoborus/boxes",
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@types/node": "13.9.2",
    "@types/tape": "4.2.34",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "rollup": "2.1.0",
    "tape": "4.13.2",
    "terser": "4.6.7",
    "ts-node": "8.8.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "weak-emitter": "5.2.1"
  }
}
